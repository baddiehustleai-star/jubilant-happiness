# Example environment variables for Photo2Profit (frontend + functions)

# Firebase (Frontend - Vite requires VITE_ prefix)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_PROJECT_ID=

# Firebase (Backend/Functions - used in api/firebaseAdmin.js)
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=

# Stripe (optional)
STRIPE_SECRET_KEY=
STRIPE_PRICE_ID=

# Stripe Webhook Secret (for webhook signature verification)
SHARED_WEBHOOK_SECRET=

# JWT Secret (for authentication)
JWT_SECRET=

# APIs (optional)
REMOVEBG_API_KEY=
EBAY_APP_ID=
EBAY_CERT_ID=
EBAY_DEV_ID=
EBAY_OAUTH_TOKEN=

# SendGrid (for weekly emails in Cloud Functions)
SENDGRID_API_KEY=
