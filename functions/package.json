{
  "name": "photo2profit-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for Photo2Profit",
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0",
    "stripe": "^14.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "openai": "^4.20.0",
    "sharp": "^0.32.6",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "typescript": "^4.9.0",
    "@types/node": "^18.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  }
}